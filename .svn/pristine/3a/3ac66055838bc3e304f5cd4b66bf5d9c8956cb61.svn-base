--회원정보
CREATE TABLE MEMBER(
    MEM_ID                VARCHAR2(20)  NOT NULL,
    MEM_MAIL              VARCHAR2(50)  NOT NULL,
    MEM_PASS              VARCHAR2(20)  NOT NULL,
    MEM_GUBUN             VARCHAR2(20)  DEFAULT 'G' NOT NULL,
    MEM_STATUS            VARCHAR2(1)   DEFAULT 'Y' NOT NULL,
    MEM_REG_DATE          DATE          DEFAULT SYSDATE NOT NULL ,
  CONSTRAINT PK_MEMBER PRIMARY KEY (MEM_ID)
);
COMMENT ON TABLE  MEMBER                IS '회원정보';
COMMENT ON COLUMN MEMBER.MEM_ID         IS '아아디';
COMMENT ON COLUMN MEMBER.MEM_MAIL       IS '메일';
COMMENT ON COLUMN MEMBER.MEM_PASS       IS '비밀번호';
COMMENT ON COLUMN MEMBER.MEM_GUBUN      IS '회원구분';
COMMENT ON COLUMN MEMBER.MEM_STATUS     IS '상태';
COMMENT ON COLUMN MEMBER.MEM_REG_DATE   IS '등록일';

--그룹정보
CREATE TABLE GROUP_INFO(
    GRP_NO          NUMBER          NOT NULL,
    GRP_NAME        VARCHAR2(20)    NOT NULL,
    GRP_COMMENT     VARCHAR2(100)   NOT NULL,
    GRP_STATUS      VARCHAR2(1)     DEFAULT 'Y',
    GRP_REG_MEM_ID  VARCHAR2(20)    NOT NULL,
    GRP_REG_DATE    DATE            DEFAULT SYSDATE,    
    CONSTRAINT PK_GROUP_INFO PRIMARY KEY (GRP_NO)      
);
COMMENT ON TABLE  GROUP_INFO                IS '그룹정보';
COMMENT ON COLUMN GROUP_INFO.GRP_NO         IS '그룹 NO';
COMMENT ON COLUMN GROUP_INFO.GRP_NAME       IS '그룹명';
COMMENT ON COLUMN GROUP_INFO.GRP_COMMENT    IS '그룹 코멘트';
COMMENT ON COLUMN GROUP_INFO.GRP_STATUS     IS '그룹 상태';
COMMENT ON COLUMN GROUP_INFO.GRP_REG_MEM_ID IS '생성자';
COMMENT ON COLUMN GROUP_INFO.GRP_REG_DATE   IS '등록일';

-- 그룹 회원 관계 정보
CREATE TABLE REL_GRP_MEM(
    REL_NO         NUMBER          NOT NULL,
    REL_GRP_NO     NUMBER          NOT NULL,
    REL_MEM_ID     VARCHAR2(20)    NOT NULL,
    REL_STATUS     VARCHAR2(1)     NOT NULL,
  CONSTRAINT PK_REL_GRP_MEM PRIMARY KEY (REL_NO),
  CONSTRAINT FK_REL_GRP_MEM1 FOREIGN KEY (REL_GRP_NO) REFERENCES GROUP_INFO (GRP_NO)
  ON DELETE CASCADE,
  CONSTRAINT FK_REL_GRP_MEM2 FOREIGN KEY (REL_MEM_ID) REFERENCES MEMBER (MEM_ID)
  ON DELETE CASCADE
);    
COMMENT ON TABLE  REL_GRP_MEM               IS '그룹 회원 관계 정보';
COMMENT ON COLUMN REL_GRP_MEM.REL_NO         IS '그룹 회원 관계 NO';
COMMENT ON COLUMN REL_GRP_MEM.REL_GRP_NO     IS '그룹 NO';
COMMENT ON COLUMN REL_GRP_MEM.REL_MEM_ID     IS '회원 ID';
COMMENT ON COLUMN REL_GRP_MEM.REL_STATUS     IS '그룹 수락 상태';
    
