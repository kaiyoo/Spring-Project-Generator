	<section id="section" class="container">
		<div class="col-xs-12 col-sm-7">
			<div class="tab-wrap">
				<div class="media">
					<div class="parrent pull-left">
						<ul class="nav nav-tabs nav-stacked">
							<li class="active"><a href="board.html">Board</a></li>
							<li class=""><a href="notice.html">Notice</a></li>
							<li class=""><a href="qna.html">QnA</a></li>
							<li class=""><a href="archive.html">Archive</a></li>
						</ul>
					</div>
				</div>
			</div>
		</div>
		<form method="post" name="boardForm" action="/submit"
		   enctype="multipart/form-data" class="col-xs-12 col-sm-99">
		      <div class="form_table">
		         <table border="1" cellspacing="0" summary="게시판 작성." class="board-form solid center">
		            <tbody>
		               <tr>
		                  <th scope="row">제 목</th>
		                  <td>
		                     <div class="item">
		                        <input type="hidden" name="bo_writer" value="${LOGIN_MEMBERINFO.mem_id}"/>
		                        <input type="hidden" name="bo_ip" value="${pageContext.request.remoteAddr}"/>
		                        <input class="i_text" title="제목" type="text" name="bo_title" />
		                     </div>
		                  </td>
		               </tr>
		               <tr>
		                  <th scope="row">작성자 대화명</th>
		                  <td>
		                     <div class="item">
		                        <input class="i_text" title="작성자대화명" type="text"  name="bo_nickname">
		                     </div>
		                  </td>
		               </tr>
		               <tr>
		                  <th scope="row">패스워드</th>
		                  <td>
		                     <div class="item">
		                        <input class="i_text" title="패스워드" type="password" name="bo_pwd">
		                     </div>
		                  </td>
		               </tr>
		               <tr>
		                  <th scope="row">이메일</th>
		                  <td>
		                     <div class="item">
		                        <input class="i_text"  title="이메일" type="text" name="bo_mail">
		                     </div>
		                  </td>
		               </tr>
		               <tr>
		                  <th scope="row">내용</th>
		                  <td>
		                     <div class="item">
		                        <textarea name="ir1" id="ir1" rows="10" style="width:100%; border: 0;"></textarea>
		                     </div>
		                  </td>
		               </tr>
		               <tr>
		                  <th scope="row">첨부파일</th>
		                  <td>
		                     <div class="item">
		                        <input type="file" name="files" class="i_file"><br/>
		                     </div>
		                     <div class="item">
		                        <input type="file" name="files" class="i_file">
		                     </div>
		                  </td>
		               </tr>
		            </tbody>
		         </table>
		      </div>
		   <div class="button_set center">
		      <input class="btn_view" type="button" value="등록"/>
		      <input class="btn_view" type="reset" value="취소"/>
		      <input class="btn_view" type="button" value="목록"/>
		   </div>
		</form>
	</section>
